						class="hero-photo hero-photo-base"
						width={320}
						height={320}
						widths={[173, 220, 320]}
						sizes="(max-width: 640px) 173px, (max-width: 900px) 220px, 320px"
						format="avif"
						quality={70}
						loading="eager"
						decoding="async"
						fetchpriority="high"
					/>
					<Image
						src={heroPhotoHover}
						alt=""
						aria-hidden="true"
						class="hero-photo hero-photo-hover"
						width={320}
						height={320}
						widths={[173, 220, 320]}
						sizes="(max-width: 640px) 173px, (max-width: 900px) 220px, 320px"
						format="avif"
						quality={56}
						loading="lazy"
						decoding="async"
					/>
				</figure>
